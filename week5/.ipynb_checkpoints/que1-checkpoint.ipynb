   "source": [
    "def is_primer(num):\n",
    "    if num == 2:\n",
    "        print(\"a primer\")\n",
    "        return\n",
    "    for i in range(2,int(num**0.5)+2):\n",
    "        if num % i == 0:\n",
    "            print(\"not a primer\")\n",
    "            return\n",
    "print(\"a primer\")\n",
    "num = int(input(\"输入整数:\"))\n",
    "is_primer(num)`"
   ]
