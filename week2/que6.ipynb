   "source": [
    "def newton(m):\n",
    "    c = 2\n",
    "    g = c / m\n",
    "    while(abs( g * g - c) > 0.0000000000001):\n",
    "        g = (g + c/g)/2\n",
    "    print('g设为c/%d的运行结果为:%.4f'%(m,g))\n",
    "newton(2)\n",
    "newton(1)\n",
    "newton(4)"
   ]
