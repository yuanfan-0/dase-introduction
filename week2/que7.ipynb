    "def newton(c):\n",
    "    g = c / 2\n",
    "    while(abs( g ** 3 - c) > 0.0000000000001):\n",
    "        g = (2*g + c/(g**2))/3\n",
    "    print('c设为%d的运行结果为:%.4f'%(c,g))\n",
    "newton(10)"
   ]
