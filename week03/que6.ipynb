    "def evaluate_grade( score ):\n",
    "    if score < 60:\n",
    "        print(\"不合格\")\n",
    "    elif score>=60 and score < 75:\n",
    "        print(\"合格\")\n",
    "    elif score >= 75 and score <90:\n",
    "        print(\"良好\")\n",
    "    else:\n",
    "        print(\"优秀\")\n",
    "score = int(input(\"请输入分数:\"))\n",
    "print(\"评定等级：\")\n",
    "evaluate_grade(score)"
   ]
