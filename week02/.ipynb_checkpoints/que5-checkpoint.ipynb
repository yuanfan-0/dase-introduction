   "source": [
    "def newton(c):\n",
    "    g = c / 2\n",
    "    while(abs( g * g - c) > 0.0000001):\n",
    "        g = (g + c/g)/2\n",
    "    print('c设为%d的运行结果为:%.4f'%(c,g))\n",
    "newton(2)\n",
    "newton(2000)"
   ]
